@import java.util.Map
@args (String handlerPackage, String host, String service, Map<String, Object> item)
package @handlerPackage;

import com.networknt.utility.NioUtils;
import com.networknt.rpc.Handler;
import com.networknt.rpc.router.ServiceHandler;
import java.nio.ByteBuffer;

@@ServiceHandler(id="@host/@service/@item.get("name")/@item.get("version")")
public class @item.get("handler") implements Handler {
    @@Override
    public ByteBuffer handle(Object input)  {
        return NioUtils.toByteBuffer("{\"message\":\"Hello World!\"}");
    }
}
